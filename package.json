{
  "name": "gpt-aigc-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:extension": "npm-watch",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "build:extension": "next build && next export && npm run lint:build",
    "lint:build": "mv out/_next out/assets && gsed -i 's/\\/_next/\\/assets/g' out/**.html",
    "package": "npm run build:extension && zip -r packages/$(date -u +\"%Y-%m-%dT%H:%M:%SZ\").zip out"
  },
  "dependencies": {
    "@codemirror/lang-javascript": "^6.1.7",
    "@uiw/react-codemirror": "^4.19.16",
    "@vercel/analytics": "^1.0.0",
    "cors": "^2.8.5",
    "next": "^13.1.1",
    "npm-watch": "^0.11.0",
    "openai": "^3.1.0",
    "react": "^18.2.0",
    "react-bootstrap": "^2.8.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.14.1",
    "react-step-progress-bar": "^1.0.3"
  },
  "engines": {
    "node": ">=14.6.0"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.14",
    "postcss": "^8.4.23",
    "tailwindcss": "^3.3.1"
  },
  "watch": {
    "build": {
      "patterns": [
        ".",
        "styles/**",
        "pages/**",
        "public/**"
      ],
      "ignore": [
        "out",
        "node_modules",
        ".next"
      ],
      "extensions": [
        "js",
        "json",
        "lock",
        "tsx",
        "ts"
      ]
    }
  }
}
